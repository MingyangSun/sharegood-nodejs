<div class="row">
  <div class="panel panel-default">
    <div class="panel-body">
      <img src="../img/<%= photo[0]["image_uri"] %>.jpg" >
      <div class="clearfix"></div>
      <hr>
      <div class="panel-heading"><a href="#" class="pull-right">Like:<%= photo[0]["like_photo"] %></a> <h4><%= photo[0]["description"] %></h4></div>
      <div class="panel-body">
      <% for(var i = 0; i < photo.length; i++) { %>
      <p><%= photo[i]["comments"] %></p>
      <% } %>
      </div>
      <hr>
      <form>
      <div class="input-group">
        <div class="input-group-btn">
        <button class="btn btn-default"><i class="glyphicon glyphicon-share"></i></button>
        </div>
        <input type="text" class="form-control" placeholder="Add a comment..">
      </div>
      </form>
    </div>
  </div>
</div>